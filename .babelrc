{
    "presets": [
        [
            "@babel/preset-react",
            {
                "pragma": "h", // default pragma is React.createElement
                "pragmaFrag": "DomFrag", // default is React.Fragment
                "throwIfNamespace": false // defaults to true
            }
        ],
        [
            "@babel/env",
            {
                "targets": {
                    "chrome": "58",
                    "ie": "11",
                    "edge": "17",
                    "firefox": "60",
                    "safari": "11.1"
                }
            }
        ]
    ],
    "plugins": ["@babel/plugin-transform-classes"]
}
